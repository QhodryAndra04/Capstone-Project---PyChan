<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <!-- Warna latar belakang lingkaran -->
    <solid android:color="#E0E0E0" />

    <!-- Garis tepi lingkaran (opsional) -->
    <stroke
        android:width="2dp"
        android:color="#B0BEC5" />

    <!-- Margin dalam lingkaran (opsional) -->
    <padding
        android:left="4dp"
        android:top="4dp"
        android:right="4dp"
        android:bottom="4dp" />
</shape>
